import axios from "axios";
const REST_API_BASE_URL = "localhost:8080/fabric/category";

export const listCategory = () => axios.get(REST_API_BASE_URL);

export const createCategory = (category) => axios.post(REST_API_BASE_URL,category);

export const updateCategory = (category) =>axios.update(REST_API_BASE_URL,category);

export const deleteCategory = () =>axios.delete(REST_API_BASE_URL);