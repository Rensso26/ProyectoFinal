import axios from "axios";
const REST_API_BASE_URL = "localhost:8080/fabric/toys";

export const listToys = () => axios.get(REST_API_BASE_URL);

export const createToys = (toys) => axios.post(REST_API_BASE_URL,toys);

export const updateToys = (toys) =>axios.update(REST_API_BASE_URL,toys);

export const deleteToys = () =>axios.delete(REST_API_BASE_URL);